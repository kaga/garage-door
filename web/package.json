{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "postinstall": "typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "clean": "rm -rf dist/",
    "build-production-workaround-rc5-bug": "npm run clean && ng build",
    "build-production": "npm run clean && ng build -prod && npm run build-production-manifest && npm run build-production-html && npm run build-production-rename",
    "build-production-manifest": "./node_modules/appcache-manifest/bin/appcache-manifest.js \"dist/*.{html,css,js}\" \"dist/app/**/*.{html,css,js}\" \"dist/vendor/es6-shim/*.js\" \"dist/vendor/reflect-metadata/*.js\" \"dist/vendor/systemjs/**/*.js\" \"dist/vendor/zone.js/dist/*.js\" -v --network-star -o dist/index.appcache",
    "build-production-html": "./node_modules/appcache-manifest/bin/appcache-manifest-fixer.js dist/index.html -o dist/mobile.html",
    "build-production-rename": "mv dist/index.html dist/_index.html && mv dist/mobile.html dist/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/router": "3.0.0-beta.2",
    "@angular2-material/button": "^2.0.0-alpha.7-3",
    "@angular2-material/card": "^2.0.0-alpha.7-3",
    "@angular2-material/core": "^2.0.0-alpha.7-3",
    "@angular2-material/icon": "^2.0.0-alpha.7-3",
    "@angular2-material/list": "^2.0.0-alpha.7-3",
    "@angular2-material/toolbar": "^2.0.0-alpha.7-3",
    "appcache-manifest": "^1.0.4",
    "es6-shim": "0.35.1",
    "font-awesome": "^4.6.3",
    "moment": "^2.14.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "when": "^3.7.7",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.10",
    "codelyzer": "0.0.20",
    "ember-cli-inject-live-reload": "1.4.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-jasmine": "0.3.8",
    "protractor": "3.3.0",
    "ts-node": "0.5.5",
    "tslint": "3.11.0",
    "typescript": "1.8.10",
    "typings": "1.3.1"
  }
}
